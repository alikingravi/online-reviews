{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions:": [
    {
      "name": "client",
      "image": "alikingravi/online-reviews-client",
      "hostname": "frontend",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "alikingravi/online-reviews-server",
      "hostname": "backend",
      "essential": false,
      "memory": 126
    },
    {
      "name": "nginx",
      "image": "alikingravi/online-reviews-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 128
    }
  ]
}
